{
    "name": "ai-agent-ide-context-sync-vscode",
    "displayName": "AI Agent IDE Context Sync",
    "description": "AI Kernel para desenvolvedores: personas, tasks, Pomodoro, Kanban, analytics e contexto unificado para m√∫ltiplos IDEs e agentes. Sistema Operacional para sua IA de desenvolvimento ficar sempre sincronizada com o git do projeto.",
    "icon": "resources/ai.png",
    "version": "2.0.22",
    "engines": {
        "vscode": "^1.80.0"
    },
    "categories": [
        "Other",
        "Chat",
        "AI",
        "Data Science",
        "Machine Learning"
    ],
    "publisher": "junio-de-almeida-vitorino",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/anarkaike/ai-agent-ide-context-sync.git",
        "directory": "packages/extension"
    },
    "activationEvents": [
        "onView:ai-agent-sync-personas",
        "onView:ai-agent-sync-analytics",
        "onView:ai-agent-sync-status",
        "onView:ai-agent-sync-timer",
        "onView:ai-agent-sync-automation"
    ],
    "main": "./extension.js",
    "contributes": {
        "commands": [
            {
                "command": "ai-agent-sync.init",
                "title": "AI Agent Sync: Initialize Workspace",
                "icon": "$(rocket)"
            },
            {
                "command": "ai-agent-sync.build",
                "title": "AI Agent Sync: Build Context",
                "icon": "$(sync)"
            },
            {
                "command": "ai-agent-sync.status",
                "title": "AI Agent Sync: Show Status"
            },
            {
                "command": "ai-agent-sync.createPersona",
                "title": "Create New Persona\nCria um novo agente de IA",
                "icon": "$(add)"
            },
            {
                "command": "ai-agent-sync.createTask",
                "title": "Create New Task\nCria uma nova task para a persona",
                "icon": "$(add)"
            },
            {
                "command": "ai-agent-sync.editPersona",
                "title": "Edit Persona\nEdita nome e descri√ß√£o da persona",
                "icon": "$(edit)"
            },
            {
                "command": "ai-agent-sync.deletePersona",
                "title": "Delete Persona\nRemove a persona do workspace",
                "icon": "$(trash)"
            },
            {
                "command": "ai-agent-sync.archiveTask",
                "title": "Archive Task\nMove a task para o arquivo",
                "icon": "$(archive)"
            },
            {
                "command": "ai-agent-sync.deleteTask",
                "title": "Delete Task\nExclui a task permanentemente",
                "icon": "$(trash)"
            },
            {
                "command": "ai-agent-sync.toggleChecklistItem",
                "title": "Toggle Checklist Item"
            },
            {
                "command": "ai-agent-sync.openFile",
                "title": "Open File"
            },
            {
                "command": "ai-agent-sync.viewPersonaDetails",
                "title": "View Full Details\nAbre o arquivo completo da persona",
                "icon": "$(eye)"
            },
            {
                "command": "ai-agent-sync.showTaskDetails",
                "title": "AI Agent Sync: Show Task Details",
                "icon": "$(info)"
            },
            {
                "command": "ai-agent-sync.showKernelInfo",
                "title": "AI Agent Sync: Kernel Details",
                "icon": "$(info)"
            },
            {
                "command": "ai-agent-sync.refreshPersonas",
                "title": "Refresh",
                "icon": "$(refresh)"
            },
            {
                "command": "ai-agent-sync.refreshStatus",
                "title": "Refresh",
                "icon": "$(refresh)"
            },
            {
                "command": "ai-agent-sync.refreshAnalytics",
                "title": "Refresh",
                "icon": "$(refresh)"
            },
            {
                "command": "ai-agent-sync.quickPicker",
                "title": "AI Agent Sync: Quick Task Picker",
                "icon": "$(search)"
            },
            {
                "command": "ai-agent-sync.searchTasks",
                "title": "AI Agent Sync: Search in Tasks",
                "icon": "$(search)"
            },
            {
                "command": "ai-agent-sync.clearActiveTask",
                "title": "AI Agent Sync: Clear Active Task"
            },
            {
                "command": "ai-agent-sync.openDashboard",
                "title": "AI Agent Sync: Open Dashboard",
                "icon": "$(graph)"
            },
            {
                "command": "ai-agent-sync.timerMenu",
                "title": "AI Agent Sync: Timer Menu",
                "icon": "$(watch)"
            },
            {
                "command": "ai-agent-sync.exportTasks",
                "title": "AI Agent Sync: Export Tasks",
                "icon": "$(export)"
            },
            {
                "command": "ai-agent-sync.customizePersona",
                "title": "Customize Persona",
                "icon": "$(paintcan)"
            },
            {
                "command": "ai-agent-sync.openKanban",
                "title": "AI Agent Sync: Open Kanban Board",
                "icon": "$(project)"
            },
            {
                "command": "ai-agent-sync.selectTheme",
                "title": "AI Agent Sync: Select Theme",
                "icon": "$(symbol-color)"
            },
            {
                "command": "ai-agent-sync.weeklyReport",
                "title": "AI Agent Sync: Weekly Report",
                "icon": "$(graph-line)"
            },
            {
                "command": "ai-agent-sync.monthlyReport",
                "title": "AI Agent Sync: Monthly Report",
                "icon": "$(calendar)"
            },
            {
                "command": "ai-agent-sync.cloudSync",
                "title": "AI Agent Sync: Cloud Sync",
                "icon": "$(cloud)"
            },
            {
                "command": "ai-agent-sync.exportBackup",
                "title": "AI Agent Sync: Export Backup",
                "icon": "$(database)"
            },
            {
                "command": "ai-agent-sync.showActiveTasks",
                "title": "AI Agent Sync: Show Active Tasks",
                "icon": "$(flame)"
            },
            {
                "command": "ai-agent-sync.showPersonas",
                "title": "AI Agent Sync: Show Personas",
                "icon": "$(account)"
            },
            {
                "command": "ai-agent-sync.showChecklistItems",
                "title": "AI Agent Sync: Show Checklist Items",
                "icon": "$(checklist)"
            },
            {
                "command": "ai-agent-sync.showCompletionDetails",
                "title": "AI Agent Sync: Completion Details",
                "icon": "$(pie-chart)"
            },
            {
                "command": "ai-agent-sync.showHeuristics",
                "title": "AI Agent Sync: Show Heuristics",
                "icon": "$(lightbulb)"
            },
            {
                "command": "ai-agent-sync.generatePrompt",
                "title": "AI Agent Sync: Generate Smart Prompt",
                "icon": "$(sparkle)"
            },
            {
                "command": "ai-agent-sync.runWorkflow",
                "title": "AI Agent Sync: Run Workflow",
                "icon": "$(play)"
            },
            {
                "command": "ai-agent-sync.runWorkflowInput",
                "title": "AI Agent Sync: Run Workflow (Input)",
                "icon": "$(play)"
            },
            {
                "command": "ai-agent-sync.scanDocs",
                "title": "Scan Documentation (Placeholders)",
                "icon": "$(search)"
            },
            {
                "command": "ai-agent-sync.ritual",
                "title": "Run Ritual (Maintenance)",
                "icon": "$(tools)"
            },
            {
                "command": "ai-agent-sync.evolve",
                "title": "Evolve Rules (Drift Detection)",
                "icon": "$(graph)"
            }
        ],
        "viewsContainers": {
            "activitybar": [
                {
                    "id": "ai-agent-sync",
                    "title": "AI Agent Sync",
                    "icon": "resources/icon.svg"
                }
            ]
        },
        "views": {
            "ai-agent-sync": [
                {
                    "id": "ai-agent-sync-personas",
                    "name": "üë• AI Agents & Tasks"
                },
                {
                    "id": "ai-agent-sync-analytics",
                    "name": "üìä Analytics"
                },
                {
                    "id": "ai-agent-sync-status",
                    "name": "‚öôÔ∏è Kernel Status"
                },
                {
                    "id": "ai-agent-sync-timer",
                    "name": "‚è±Ô∏è Pomodoro & Focus"
                },
                {
                    "id": "ai-agent-sync-automation",
                    "name": "‚ö° Automation & AI"
                }
            ]
        },
        "menus": {
            "view/title": [
                {
                    "command": "ai-agent-sync.init",
                    "when": "view == ai-agent-sync-personas",
                    "group": "navigation@1"
                },
                {
                    "command": "ai-agent-sync.build",
                    "when": "view == ai-agent-sync-personas",
                    "group": "navigation@2"
                },
                {
                    "command": "ai-agent-sync.createPersona",
                    "when": "view == ai-agent-sync-personas",
                    "group": "navigation@3"
                },
                {
                    "command": "ai-agent-sync.createTask",
                    "when": "view == ai-agent-sync-personas",
                    "group": "navigation@3.5"
                },
                {
                    "command": "ai-agent-sync.openKanban",
                    "when": "view == ai-agent-sync-personas",
                    "group": "navigation@4"
                },
                {
                    "command": "ai-agent-sync.refreshPersonas",
                    "when": "view == ai-agent-sync-personas",
                    "group": "navigation@5"
                },
                {
                    "command": "ai-agent-sync.searchTasks",
                    "when": "view == ai-agent-sync-personas",
                    "group": "navigation@6"
                },
                {
                    "command": "ai-agent-sync.exportTasks",
                    "when": "view == ai-agent-sync-personas",
                    "group": "navigation@7"
                },
                {
                    "command": "ai-agent-sync.refreshStatus",
                    "when": "view == ai-agent-sync-status",
                    "group": "navigation"
                },
                {
                    "command": "ai-agent-sync.refreshAnalytics",
                    "when": "view == ai-agent-sync-analytics",
                    "group": "navigation"
                }
            ],
            "view/item/context": [
                {
                    "command": "ai-agent-sync.editPersona",
                    "when": "view == ai-agent-sync-personas && viewItem == persona",
                    "group": "inline@1"
                },
                {
                    "command": "ai-agent-sync.deletePersona",
                    "when": "view == ai-agent-sync-personas && viewItem == persona",
                    "group": "inline@2"
                },
                {
                    "command": "ai-agent-sync.customizePersona",
                    "when": "view == ai-agent-sync-personas && viewItem == persona",
                    "group": "inline@3"
                },
                {
                    "command": "ai-agent-sync.viewPersonaDetails",
                    "when": "view == ai-agent-sync-personas && viewItem == persona",
                    "group": "inline@4"
                },
                {
                    "command": "ai-agent-sync.createTask",
                    "when": "view == ai-agent-sync-personas && viewItem == tasks-section",
                    "group": "inline"
                },
                {
                    "command": "ai-agent-sync.archiveTask",
                    "when": "view == ai-agent-sync-personas && viewItem == task",
                    "group": "inline@1"
                },
                {
                    "command": "ai-agent-sync.deleteTask",
                    "when": "view == ai-agent-sync-personas && viewItem == task",
                    "group": "inline@2"
                }
            ]
        },
        "keybindings": [
            {
                "command": "ai-agent-sync.quickPicker",
                "key": "ctrl+shift+t",
                "mac": "cmd+shift+t",
                "when": "!terminalFocus"
            },
            {
                "command": "ai-agent-sync.searchTasks",
                "key": "ctrl+shift+f",
                "mac": "cmd+shift+f",
                "when": "!searchViewletVisible"
            }
        ]
    },
    "scripts": {
        "lint": "eslint .",
        "pretest": "npm run lint",
        "test": "node ./test/runTest.js",
        "test:unit": "mocha --timeout 10000 test/unit/**/*.test.js",
        "test:coverage": "nyc --reporter=text --reporter=html npm run test:unit",
        "package": "vsce package --no-dependencies",
        "publish": "vsce publish"
    },
    "devDependencies": {
        "@types/mocha": "^10.0.0",
        "@types/node": "16.x",
        "@types/vscode": "^1.80.0",
        "@vscode/test-electron": "^2.3.0",
        "eslint": "^8.41.0",
        "mocha": "^10.2.0",
        "nyc": "^17.1.0",
        "proxyquire": "^2.1.3",
        "sinon": "^21.0.1"
    }
}
